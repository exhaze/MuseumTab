<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
	<servlet-name>CreateExhibit</servlet-name>
	<servlet-class>com.jes.museumtab.backend.ServletCreateExhibit</servlet-class>
	</servlet>
	<servlet>
	<servlet-name>PushExhibitUpdate</servlet-name>
	<servlet-class>com.jes.museumtab.backend.ServletPushUpdate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateExhibit</servlet-name>
		<url-pattern>/new</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PushExhibitUpdate</servlet-name>
		<url-pattern>/update/*</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>MuseumApp.jsp</welcome-file>
	</welcome-file-list>
</web-app>
